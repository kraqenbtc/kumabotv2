    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "sqlite3": "^5.1.6",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.7",
