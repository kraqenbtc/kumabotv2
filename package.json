    "api:dev": "nodemon --exec ts-node src/api/index.ts",
    "bot": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "delegated-key": "ts-node src/cli/delegated-key.ts"
