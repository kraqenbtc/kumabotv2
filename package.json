{
  "name": "kumbo",
  "version": "2.0.0",
  "description": "Kumbo - Advanced Grid Trading Bot for Kuma Exchange",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "api": "ts-node src/api/index.ts",
    "api:dev": "nodemon --exec ts-node src/api/index.ts",
    "bot": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "grid-trading",
    "crypto",
    "trading-bot",
    "kuma-exchange",
    "kumbo"
  ],
  "author": "",
